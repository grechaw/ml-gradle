{
  "role-name": "sample-project-role",
  "description": "sample-project rest-admin role that allows the user to insert documents via XCC and clear the database at the start of a JUnit test",
  "role": ["rest-admin", "trigger-management"],
  "privilege": [
    {
      "privilege-name": "create-trigger",
      "action": "http://marklogic.com/xdmp/privileges/create-trigger",
      "kind": "execute"
    },
    {
      "privilege-name": "xdmp:eval",
      "action": "http://marklogic.com/xdmp/privileges/xdmp-eval",
      "kind": "execute"
    },
    {
      "privilege-name": "xdmp:eval-in",
      "action": "http://marklogic.com/xdmp/privileges/xdmp-eval-in",
      "kind": "execute"
    },
    {
      "privilege-name": "xdbc:eval-in",
      "action": "http://marklogic.com/xdmp/privileges/xdbc-eval-in",
      "kind": "execute"
    },
    {
      "privilege-name": "xdbc:insert-in",
      "action": "http://marklogic.com/xdmp/privileges/xdbc-insert-in",
      "kind": "execute"
    },
    {
      "privilege-name": "any-uri",
      "action": "http://marklogic.com/xdmp/privileges/any-uri",
      "kind": "execute"
    },
    {
      "privilege-name": "xdmp:forest-clear",
      "action": "http://marklogic.com/xdmp/privileges/xdmp-forest-clear",
      "kind": "execute"
    }
  ]
}